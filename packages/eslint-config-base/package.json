{
  "name": "@cospired/eslint-config-base",
  "version": "1.0.0",
  "description": "Slightly opinionated eslint config based on airbnb's config",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "posttravis": ":",
    "prepublish": "(in-install || eslint-find-rules --unused) && (not-in-publish || npm test) && safe-publish-latest",
    "pretest": "npm run --silent lint",
    "pretests-only": "node ./test/requires",
    "pretravis": ":",
    "test": "npm run --silent tests-only",
    "tests-only": "babel-tape-runner ./test/*.test.js",
    "travis": "npm run --silent test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cospired/javascript"
  },
  "contributors": [
    {
      "name": "Dirk Haage",
      "email": "dirk@cospired.com",
      "url": "https://cospired.com/"
    },
    {
      "name": "Jan Schmidle",
      "email": "jan@cospired.com",
      "url": "https://cospired.com/"
    }
  ],
  "author": "Dirk Haage <dirk@cospired.com>",
  "license": "BSD-3-Clause-Clear",
  "dependencies": {
    "eslint-config-airbnb-base": "^13.1.0",
    "object.assign": "^4.1.0"
  },
  "devDependencies": {
    "babel-preset-airbnb": "^3.2.1",
    "babel-tape-runner": "^3.0.0",
    "eslint": "^5.15.0",
    "eslint-find-rules": "^3.3.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-hapi": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "in-publish": "^2.0.0",
    "safe-publish-latest": "^1.1.2",
    "tape": "^4.10.1"
  },
  "peerDependencies": {
    "eslint": "^5.15.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-hapi": "^4.1.0",
    "eslint-plugin-import": "^2.16.0"
  }
}
