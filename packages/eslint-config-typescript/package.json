{
  "name": "@cospired/eslint-config-typescript",
  "version": "6.0.0",
  "description": "Slightly opinionated eslint config for typescript based on airbnb's config",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --report-unused-disable-directives .",
    "prepublishOnly": "eslint-find-rules --unused && npm test && safe-publish-latest",
    "prepublish": "not-in-publish || npm run prepublishOnly",
    "pretest": "npm run --silent lint",
    "pretests-only": "node ./test/requires",
    "test": "npm run --silent tests-only",
    "tests-only": "babel-tape-runner ./test/*.test.js",
    "pretravis": ":",
    "travis": "npm run --silent test",
    "posttravis": ":"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cospired/javascript"
  },
  "contributors": [
    {
      "name": "Dirk Haage",
      "email": "dirk@cospired.com",
      "url": "https://cospired.com/"
    }
  ],
  "author": "Dirk Haage <dirk@cospired.com>",
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "cospired",
    "javascript",
    "styleguide",
    "typescript",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "license": "BSD-3-Clause-Clear",
  "dependencies": {
    "@cospired/eslint-config-base": "^6.0.0",
    "eslint-config-airbnb-typescript": "^16.1.2",
    "object.assign": "^4.1.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-plugin-import": "^2.25.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.17.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-preset-airbnb": "^5.0.0",
    "babel-tape-runner": "^3.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.25.4",
    "in-publish": "^2.0.1",
    "safe-publish-latest": "^2.0.0",
    "tape": "^5.5.2"
  }
}
